<%- include("./_nav.ejs") %>
    <main>
        <div class="container">
            <div class="art_login_form_co">

                <form class="contacthero__content__form" id="contact_us_form" action="<%= path %>users/login" method="POST">
                    <!-- <label for="user_name"><%- lang_(`Your_username`) %></label> -->
                    <div class="art_input_co ">
                        <input type="text" placeholder="<%- lang_(`Email`) %> " id="user_name" name="user_name" class="art_input ">
                    </div>
                    <!-- <label for="password"><%- lang_(`Password`) %></label> -->
                    <div class="art_input_co ">
                        <input type="password" placeholder="<%- lang_(`Password`) %> " id="password" name="password" class="art_input ">
                    </div>
                    <div class="art_forgot">
                        <a href="forgot" class="art_link">
                            <%- lang_(`Forgot_password?`) %>
                        </a>
                    </div>


                    <div class="art_submit_btn_co ">
                        <button type="sumit" name="art_sumit" class="art_btn art_gree_btn filled waves-effect "><%- lang_(`Login`) %></button>
                    </div>
                </form>
            </div>
        </div>
    </main>